<Window x:Class="DB_CRUD.Eliminar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DB_CRUD"
        mc:Ignorable="d"
        Title="Eliminar" Height="450" Width="450">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Mercado/Guardar.jpg"/>
        </Grid.Background>
        <Label x:Name="lbEliminar" Content="Jugador a Eliminar" HorizontalAlignment="Left" Margin="32,31,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.334,-2.942" Foreground="#FFFFF0F0" FontFamily="Arial" FontSize="22"/>
        <TextBox x:Name="txtBEliminar" HorizontalAlignment="Left" Height="35" Margin="32,71,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="294"/>
        <Button x:Name="btnEliminar" Content="Eliminar" HorizontalAlignment="Left" Margin="32,146,0,0" VerticalAlignment="Top" Width="88" RenderTransformOrigin="-1.122,0.568" Height="37" FontSize="18" Click="BtnEliminar_Click"/>

    </Grid>
</Window>
